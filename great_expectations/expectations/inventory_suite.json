{
  "expectation_suite_name": "inventory_suite",
  "expectations": [
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": { "column": "item_id" }
    },
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": { "column": "item_name" }
    },
    {
      "expectation_type": "expect_column_values_to_be_between",
      "kwargs": { "column": "quantity", "min_value": 0.1 }
    },
    {
      "expectation_type": "expect_column_values_to_be_in_set",
      "kwargs": { "column": "unit", "value_set": ["kg", "g", "L", "ml", "pcs"] }
    },
    {
      "expectation_type": "expect_column_values_to_match_strftime_format",
      "kwargs": { "column": "expiry_date", "strftime_format": "%Y-%m-%d" }
    },
    {
      "expectation_type": "expect_column_values_to_be_between",
      "kwargs": { "column": "reorder_threshold", "min_value": 0 }
    },
    {
      "expectation_type": "expect_table_row_count_to_be_between",
      "kwargs": { "min_value": 100, "max_value": 5000 }
    }
  ],
  "meta": { "great_expectations_version": "1.2.4" }
}